template(name="selectTeam")
  .row
    .col-md-4.col-md-offset-3
      block content
        +inputAutocomplete settings=settings id="team_input" class="form-control input-lg" placeholder="Team Name:"

    .col-md-2
      button#select_team_button.btn.btn-primary.btn-lg.btn-block Select Team
  .well
    .row.players
      each team.players
        span.col-md-3.label.label-default {{ fname }} {{ lname }} {{ number }}
